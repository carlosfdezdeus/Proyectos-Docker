function DebugLog(text) {
    if (window.debug == true) {
        console.log(text);
    }
}

function DebugInfo(text) {
    if (window.debug == true) {
        console.info(text);
    }
}

function DebugWarn(text) {
    if (window.debug == true) {
        console.warn(text);
    }
}