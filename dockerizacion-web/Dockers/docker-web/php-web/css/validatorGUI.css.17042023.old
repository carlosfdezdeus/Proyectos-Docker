#MenuModalCables .modal-body{   
    width: 100%; 
}



#MenuModalCables img {
  border-radius: 8px;  
}

#MenuModalCables .btn-cables {
  position: relative;
  display: inline-block;
  margin-top: 3px;
  margin-right: 3px;
  background-color:#9B9B9B;
  border-radius: 8px;
 
}
  

#MenuModalCables .btn-cables[title]:hover::after {
  content: attr(title);
  position: absolute;
  top: 0;
  left: 0;
  /*bottom: 0; */
  background-color:#000;
  color:#fff;
  font-family: Verdana;
  font-size: 0.99em;
}




